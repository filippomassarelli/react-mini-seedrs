: {\n    margin: \"10px\",\n  },\n}));\n\nexport default function FormDialog({ campaignId, multiple, campaignName }) {\n  //OTHER HOOKS\n  const classes = useStyles();\n  let history = useHistory();\n\n  //STATE HOOKS\n\n  const [open, setOpen] = useState(false);\n\n  const [inputs, setInputs] = useState({\n    campaign_id: campaignId,\n    user_name: \"\",\n    investment_amount: \"\",\n    currency: \"\",\n  });\n\n  const [investmentStatus, setInvestmentStatus] = useState({\n    isInvalid: false,\n    isSuccessful: false,\n    errorMessage: \"\",\n  });\n\n  //HANDLERS\n  const handleClickOpen = () => {\n    setOpen(true);\n  };\n\n  const handleClose = () => {\n    setOpen(false);\n  };\n\n  const handleChange = (e) => {\n    setInputs({\n      ...inputs,\n      [e.target.name]: e.target.value,\n    });\n  };\n\n  const handleInvalid = () => {\n    setInvestmentStatus({\n      ...investmentStatus,\n      isInvalid: false,\n    });\n  };\n\n  const handleSuccessful = () => {\n    setInvestmentStatus({\n      ...investmentStatus,\n      isSuccessful: false,\n    });\n    history.push(\"/\");\n  };\n\n  const handleInvest = (e) => {\n    e.preventDefault();\n    console.log(inputs);\n    investmentsAPI\n      .post(\"\", inputs)\n      .then((response) => {\n        setInvestmentStatus({ ...investmentStatus, isSuccessful: true });\n      })\n      .catch((error) => {\n        setInvestmentStatus({\n          ...investmentStatus,\n          isInvalid: true,\n          errorMessage: error.response.data.message,\n        });\n      });\n  };\n\n  const handleShare = () => {\n    // setInvestmentStatus({\n    //   ...investmentStatus,\n    //   isSuccessful: false,\n    // });\n    // history.push(\"/\");\n  };\n\n  //RETURN\n\n  return (\n    <div>\n      <Snackbar\n        open={investmentStatus.isInvalid}\n        autoHideDuration={6000}\n        onClose={handleInvalid}\n        anchorOrigin={{ vertical: \"top\", horizontal: \"center\" }}\n      >\n        <Alert onClose={handleInvalid} severity=\"error\">\n          {investmentStatus.errorMessage}\n        </Alert>\n      </Snackbar>\n\n      <Button variant=\"contained\" color=\"primary\" onClick={handleClickOpen}>\n        Invest\n      </Button>\n\n      <Dialog\n        open={open}\n        onClose={handleClose}\n        aria-labelledby=\"form-dialog-title\"\n        disableBackdropClick={investmentStatus.isSuccessful ? true : false}\n        disableEscapeKeyDown={investmentStatus.isSuccessful ? true : false}\n      >\n        <DialogTitle id=\"form-dialog-title\">\n          {investmentStatus.isSuccessful\n            ? `Thank you ${inputs.user_name},`\n            : \"Invest\"}\n        </DialogTitle>\n\n        <DialogContent>\n          {investmentStatus.isSuccessful ? (\n            <DialogContentText className={classes.formText}>\n              You successfully invested £{inputs.investment_amount} in{\" \"}\n              {campaignName} !\n            </DialogContentText>\n          ) : (\n            <div>\n              <DialogContentText className={classes.formText}>\n                For this campaign you need to invest a multiple of £{multiple}\n              </DialogContentText>\n              <TextField\n                autoFocus\n                margin=\"dense\"\n                id=\"name\"\n                name=\"user_name\"\n                label=\"Name\"\n                type=\"text\"\n                fullWidth\n                color=\"primary\"\n                onChange={handleChange}\n              />\n              <TextField\n                margin=\"dense\"\n                id=\"investment_amount\"\n                name=\"investment_amount\"\n                label=\"Investment amount\"\n                type=\"number\"\n                fullWidth\n                onChange={handleChange}\n                color=\"inherit\"\n              />\n              <CurrencySelect onChange={handleChange} />\n            </div>\n          )}\n        </DialogContent>\n        <DialogActions>\n          {investmentStatus.isSuccessful ? (\n            <div>\n              <Button\n                onClick={handleSuccessful}\n                className={(classes.formText, classes.formButtons)}\n                variant=\"outlined\"\n              >\n                All Campaigns\n              </Button>\n              {/* <Button\n                onClick={handleShare}\n                color=\"primary\"\n                variant=\"contained\"\n                className={classes.formButtons}\n              >\n                Share on Socials\n              </Button> */}\n              <SocialShare />\n            </div>\n          ) : (\n            <div>\n              <Button\n                onClick={handleClose}\n                className={(classes.formText, classes.formButtons)}\n                variant=\"outlined\"\n              >\n                Cancel\n              </Button>\n              <Button\n                onClick={handleInvest}\n                color=\"primary\"\n                variant=\"contained\"\n                className={classes.formButtons}\n              >\n                Confirm\n              </Button>\n            </div>\n          )}\n        </DialogActions>\n      </Dialog>\n    </div>\n  );\n}\n","/Users/filippomassarelli/Code/seedrs-interview/react-mini-seedrs/src/components/form/Socials.jsx",[],{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","replacedBy":"92"},{"ruleId":"89","replacedBy":"93"},{"ruleId":"91","replacedBy":"94"},{"ruleId":"95","severity":1,"message":"96","line":4,"column":8,"nodeType":"97","messageId":"98","endLine":4,"endColumn":19},{"ruleId":"89","replacedBy":"99"},{"ruleId":"91","replacedBy":"100"},{"ruleId":"89","replacedBy":"101"},{"ruleId":"91","replacedBy":"102"},{"ruleId":"95","severity":1,"message":"103","line":96,"column":9,"nodeType":"97","messageId":"98","endLine":96,"endColumn":20},"no-native-reassign",["104"],"no-negated-in-lhs",["105"],["104"],["105"],"no-unused-vars